{
    "name": "tstaxirl",
    "version": "1.0.0",
    "description": "repository wide enforcement of a style guide",
    "main": "index.js",
    "scripts": {
        "test": "cd ./packages/core; pnpm test",
        "dependencies": "pnpm install; cd ./packages/core; pnpm install; cd ../../backend; pnpm install; cd ../frontend; pnpm install",
