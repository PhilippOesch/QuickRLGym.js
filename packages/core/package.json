{
    "name": "quickrl.core",
    "version": "0.6.11",
    "description": "This is a simple framework for implementing and testing reinforcement learning environments and algorithms",
    "main": "dist/index.cjs.js",
    "module": "dist/index.es.js",
    "types": "dist/dts/index.d.ts",
    "type": "module",
    "keywords": [
        "reinforcement learning",
        "tensorflowJS",
        "javascript",
        "typescript",
        "machine learning",
        "games",
        "library"
    ],
    "scripts": {
        "test": "tsc -d -p tsconfig.test.json; nyc mocha ./distTesting/testing/**/*.test.js;",
        "release": "pnpm test; pnpm buildProd; pnpm publish --no-git-checks;",
        "coverage": "nyc report --reporter=lcov;",
        "build": "rm -r ./dist; rollup --config; npx tsc -d -p tsconfig.json"
    },
    "author": "Philipp Oeschger",
    "license": "MIT",
    "dependencies": {
        "@tensorflow/tfjs": "^4.6.0",
        "seedrandom": "^3.0.5"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.9",
        "@types/seedrandom": "^3.0.2",
        "@types/sinon": "^10.0.13",
        "mocha": "^10.2.0",
        "mocha-suppress-logs": "^0.3.1",
        "npx": "^10.2.2",
        "nyc": "^15.1.0",
        "rollup": "^3.23.0",
        "sinon": "^15.0.1",
        "taffydb": "^2.7.3",
        "tslib": "^2.5.2",
        "typescript": "^4.8.4"
    }
}
