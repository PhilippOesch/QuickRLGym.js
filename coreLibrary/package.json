{
    "name": "quickrl.core",
    "version": "0.6.11",
    "description": "This is a simple framework for implementing and testing reinforcement learning environments and algorithms",
    "main": "./out/index.js",
    "types": "./out/index.d.ts",
    "keywords": [
        "reinforcement learning",
        "javascript",
        "typescript",
        "machine learning",
        "games"
    ],
    "scripts": {
        "test": "pnpm buildDev; nyc mocha ./out/testing/*;",
        "buildProd": "rm -r ./out; npx tsc -d -p tsconfig.json",
        "buildDev": "rm -r ./out; npx tsc -d -p tsconfig.dev.json",
        "release": "pnpm test; pnpm buildProd; pnpm publish --no-git-checks;",
        "coverage": "nyc report --reporter=lcov;"
    },
    "author": "Philipp Oeschger",
    "license": "MIT",
    "dependencies": {
        "@tensorflow/tfjs": "^4.2.0",
        "seedrandom": "^3.0.5"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.9",
        "@types/seedrandom": "^3.0.2",
        "@types/sinon": "^10.0.13",
        "mocha": "^10.2.0",
        "mocha-suppress-logs": "^0.3.1",
        "npx": "^10.2.2",
        "nyc": "^15.1.0",
        "sinon": "^15.0.1",
        "typescript": "^4.8.4"
    }
}
