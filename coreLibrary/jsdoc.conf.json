{
    "recurseDepth": 10,
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["src"],
        "exclude": ["src/testing"],
        "includePattern": "\\.(jsx|js|ts|tsx)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown",
        "better-docs/typescript",
        "better-docs/category",
        "better-docs/publish",
        "better-docs/component"
    ],
    "templates": {
        "better-docs": {
            "name": "QuickRLJS"
        }
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "../README.md",
        "template": "./node_modules/better-docs"
    }
}
